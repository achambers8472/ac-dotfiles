# Process configuration
#background yes
update_interval 1

# Window options
own_window yes
own_window_type normal
#own_window_hints skip_taskbar,skip_pager
own_window_hints above,below,undecorated,skip_taskbar,skip_pager,sticky
own_window_colour 000000
own_window_argb_visual yes
own_window_argb_value 110
#alignment bottom_right
alignment middle_middle
gap_x 20
gap_y 20
maximum_size 1680 1050
double_buffer yes
use_spacer left

# Font options
use_xft yes
font Inconsolata:size=12
use_spacer left

# Size options
default_bar_size 80 8
pad_percents 1

text_buffer_size 2048
minimum_size 80


